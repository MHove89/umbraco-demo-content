{
	"sourceEntityTypes": [
		"home"
	],
	"route": {
		"handles": [
			{
				"$exp": "navigation"
			}
		]
	},
	"properties": {
		"navigationItems": {
			"type": "array",
			"input": {
				"$lookup": {
					"operator": "equals",
					"sourceEntityProperty": "originParentId",
					"matchValue": {
						"$exp": "{originId}"
					}
				}
			},
			"items": {
				"type": "reference",
				"gid": {
					"$exp": "{item.id}"
				},
				"view": "navigationItem"
			}
		}
	}
}
